{
  "name": "diceware-generator",
  "version": "1.2.2",
  "description": "A diceware generator",
  "main": "src",
  "scripts": {
    "pretest": "eslint src",
    "test": "istanbul cover _mocha",
    "coveralls": "coveralls < coverage/lcov.info"
  },
  "engines": {
    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lgaticaq/diceware-generator.git"
  },
  "keywords": [
    "diceware",
    "password",
    "generator"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgaticaq/diceware-generator/issues"
  },
  "homepage": "https://github.com/lgaticaq/diceware-generator#readme",
  "dependencies": {
    "diceware-wordlist-en": "^1.0.0",
    "diceware-wordlist-jp": "^1.0.1",
    "diceware-wordlist-sp": "^1.0.0",
    "diceware-wordlist-swe": "^1.0.1",
    "lodash": "^4.12.0",
    "secure-random": "^1.1.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.10.1",
    "istanbul": "^0.4.3",
    "js-combinatorics": "^0.5.2",
    "mocha": "^2.4.5"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [
        2,
        2
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "quotes": [
        2,
        "single"
      ],
      "semi": [
        2,
        "always"
      ]
    }
  },
  "tonicExampleFilename": "example.js"
}
